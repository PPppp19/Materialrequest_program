	<queryString>
		<![CDATA[SELECT 
    ROWNUMBER() OVER() AS NO,
    A.*
FROM (
    SELECT 
        *
    FROM  
          (
            SELECT * FROM BRLDTA0100.FAR_MTRREQ04  
            WHERE  ORD_ID =  $P{ord_id} 
            AND REG_CONO  =  $P{company_cono} 
          ) AS HEAD
    LEFT JOIN        
          (
            SELECT * FROM BRLDTA0100.FAR_ITMTLB04 fi 
            WHERE  ORD_ID =  $P{ord_id} 
            AND REG_CONO  =  $P{company_cono} 
          ) AS DETAIL 
          ON HEAD.ORD_ID  = DETAIL.ORD_ID
    LEFT JOIN
          (
              SELECT ST_N6L3 AS REQ_NAME ,ST_SIGN AS REQ_SIGN 
              FROM BRLDTA0100.STAFFLIST s 
          ) AS SIGHNATURE
          ON  HEAD.ORD_REGB = SIGHNATURE.REQ_NAME 
    LEFT JOIN
          (
              SELECT ST_N6L3 AS DEP_NAME ,ST_SIGN AS DEP_SIGN 
              FROM BRLDTA0100.STAFFLIST s 
          ) AS DEP_SIGHNATURE
          ON  HEAD.ORD_DPMH = DEP_SIGHNATURE.DEP_NAME 
    LEFT JOIN
          (
              SELECT ST_N6L3 AS MG1_NAME ,ST_SIGN AS MG1_SIGN 
              FROM BRLDTA0100.STAFFLIST s 
          ) AS MG1_SIGHNATURE
          ON  HEAD.ORD_MG1 = MG1_SIGHNATURE.MG1_NAME  
    LEFT JOIN
          (
              SELECT ST_N6L3 AS MG2_NAME ,ST_SIGN AS MG2_SIGN 
              FROM BRLDTA0100.STAFFLIST s 
          ) AS MG2_SIGHNATURE
          ON  HEAD.ORD_MG2 = MG2_SIGHNATURE.MG2_NAME
) A]]>
	</queryString>
